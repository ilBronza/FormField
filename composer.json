{
    "name": "ilbronza/formfield",
    "description": ":form_field",
    "license": "MIT",
    "authors": [
        {
            "name": "Davide Bronzini",
            "email": "davide_bronzini@hotmail.com",
            "homepage": "https://sistema.srl"
        }
    ],
    "homepage": "https://github.com/ilbronza/formfield",
    "keywords": ["Laravel", "FormField"],
    "require": {
        "illuminate/support": "~5|~6|~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "IlBronza\\FormField\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IlBronza\\FormField\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IlBronza\\FormField\\FormFieldServiceProvider"
            ],
            "aliases": {
                "FormField": "IlBronza\\FormField\\Facades\\FormField"
            }
        }
    }
}
